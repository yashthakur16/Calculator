#include<iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"Enter the input : ";
    cin>>a;
    switch (1)
    {
        case 1 :b=a/100;
                cout<<"Number of 100 rupee note required= "<<b<<endl;
                a=a-(b*100);

        case 2 :b=a/50;
                cout<<"Number of 50 rupee note required = "<<b<<endl;
                a=a-(b*50);

        case 3 :b=a/20;
                cout<<"Number of 20 rupee note required = "<<b<<endl;
                a=a-(b*20);

        case 4 :b=a/10;
                cout<<"Number of 10 rupee note required = "<<b<<endl;
                a=a-(b*10);
    }
}
